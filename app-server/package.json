{
  "name": "claexa-server",
  "version": "0.0.2",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node -r ts-node/register -r tsconfig-paths/register src/main.ts",
    "start:dev": "nest start --watch -r tsconfig-paths/register",
    "start:debug": "nest start --debug --watch -r tsconfig-paths/register",
    "start:prod": "node -r tsconfig-paths/register dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "typeorm-ts-node-commonjs --dataSource data-source.ts migration:generate",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "migration:run": "typeorm-ts-node-commonjs --dataSource data-source.ts migration:run",
    "migration:revert": "typeorm-ts-node-commonjs --dataSource data-source.ts migration:revert",
    "migration:show": "typeorm-ts-node-commonjs --dataSource data-source.ts migration:show",
    "docker:dev:up": "docker compose -f compose.dev.yaml up -d",
    "docker:dev:down": "docker compose -f compose.dev.yaml down",
    "docker:dev:logs": "docker compose -f compose.dev.yaml logs -f",
    "docker:dev:clean": "docker compose -f compose.dev.yaml down -v",
    "docker:test:up": "docker compose -f compose.test.yaml up -d --build",
    "docker:test:down": "docker compose -f compose.test.yaml down",
    "docker:test:logs": "docker compose -f compose.test.yaml logs -f",
    "docker:test:clean": "docker compose -f compose.test.yaml down -v"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.922.0",
    "@aws-sdk/s3-request-presigner": "^3.922.0",
    "@casl/ability": "^6.7.3",
    "@faker-js/faker": "^9.9.0",
    "@google-cloud/storage": "^7.17.2",
    "@grpc/grpc-js": "^1.14.0",
    "@grpc/proto-loader": "^0.8.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.4.20",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/microservices": "^10.4.20",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/typeorm": "^11.0.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.13.1",
    "axios-retry": "^4.5.0",
    "cashfree-pg": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "docx": "^9.5.1",
    "dotenv": "^17.2.3",
    "fastparallel": "^2.4.1",
    "firebase-admin": "^13.5.0",
    "form-data": "^4.0.4",
    "highlight.js": "^11.11.1",
    "joi": "^17.13.3",
    "katex": "^0.16.25",
    "marked": "^16.4.1",
    "marked-katex-extension": "^5.1.5",
    "node-tikzjax": "^1.0.3",
    "passport": "^0.7.0",
    "passport-http-bearer": "^1.0.1",
    "pg": "^8.16.3",
    "posthog-node": "^4.18.0",
    "puppeteer": "^24.27.0",
    "reflect-metadata": "^0.2.2",
    "romans": "^3.1.0",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.27",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.20",
    "@types/express": "^5.0.5",
    "@types/form-data": "^2.5.2",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.19.24",
    "@types/passport-http-bearer": "^1.0.42",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "pnpm@10.17.1"
}
